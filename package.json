{
  "name": "first",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "PORT=3000 APP_URL='http://localhost' DEBUG=projnotes node./distin/bin/www",
    "dev": "NODE_ENV=development DEBUG=projnotes::server babel-watch ./server/bin/www",
    "devc": "NODE_ENV=development webpack serve --config ./webpack.dev.config.js --mode development",
    "devb": "babel-watch ./server/bin/www",
    "cleans": "rm -rf ./dist",
    "cleanc": "rm -f ./public/bundle.js ./public/styles/app.css",
    "b:s": "npm run cleans && babel ./server --out-dir dist --copy-files",
    "b:c": "npm run cleanc && webpack --config ./webpack.config.js --mode production",
    "build": "npm run b:c && npm run b:s",
    "lint:c": "eslint ./client/**/*.js",
    "lint:s": "eslint ./server/**/*.js",
    "clean:logs": "rm -rf ./server/logs/*.log"
  },
  "dependencies": {
    "app-root-path": "^3.1.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "~4.16.1",
    "express-handlebars": "^7.1.2",
    "hbs": "~4.0.4",
    "http-errors": "~1.6.3",
    "i": "^0.3.7",
    "materialize-css": "^1.0.0-rc.2",
    "morgan": "~1.9.1",
    "supports-color": "^9.3.1",
    "webpack": "^5.76.1",
    "webpack-dev-middleware": "^6.1.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-hot-middleware": "^2.25.4",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@babel/core": "^7.22.17",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/node": "^7.22.15",
    "@babel/preset-env": "^7.22.15",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-preset-minify": "^0.5.2",
    "babel-watch": "^7.8.1",
    "core-js": "^3.32.2",
    "css-loader": "^6.8.1",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-webpack-plugin": "^4.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "prettier": "^2.8.7",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  }
}
